        <target state="translated">[Facoltativo] Se i commenti true vengono rimossi dalle istruzioni</target>
      </trans-unit>
      <trans-unit id="mysql.format.reindent.description">
        <source xml:lang="en">[Optional] If true the indentations of the statements are changed.</source>
        <target state="translated">[Facoltativo] Se true, i rientri delle istruzioni vengono modificati.</target>
      </trans-unit>
